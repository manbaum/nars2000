CFLAGS_DEBUG=/D "RESDEBUG=$(RESDEBUG)" /Ox
!endif

CFLAGS_WEXE = /nologo /Gs /GA /W3 /WX /Zp1 $(CFLAGS_DEBUG) /D $(WDEF)
LFLAGS_WEXE = /NOLOGO /MAP
