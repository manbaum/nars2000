IDR_ACCEL ACCELERATORS MOVEABLE PURE
BEGIN
    "^A",       IDM_SELECTALL
    "^C",       IDM_COPY
    "^P",       IDM_PRINT_WS
    "^V",       IDM_PASTE
    "^X",       IDM_CUT
    "^Y",       IDM_REDO
    "^Z",       IDM_UNDO
END


