            for (i = 1; i < N; i++)                                                                                 \
                lpMemRes[uRes].parts[i] = 0;                                                                        \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case  10:       /* |R                                                                                     */\
            lpMemRes[uRes].parts[0] = MagHC##N##F (lpatRht->aplHC##N##F);                                           \
                                                                                                                    \
