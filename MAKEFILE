{$(O)}.C{$(O)}.DEP:
    @Set INCLUDE=$(I98);$(H);$(O)
    @Set PATH=$(BC)
    depscan -q -s$(PROJPATH)$(O)=$$(O) %s > $@
    @Set PATH=$(OLDPATH)

{$(WS)}.C{$(O)}.DEP:
    @Set INCLUDE=$(WS).;$(WS)Include;$(WS)Include\Wine;$(WS)Include\msvcrt;.
    @Set PATH=$(BC)
    depscan -q -s$(PROJPATH)$(O)=$$(O),$(WS)=$$(WS) %s > $@
    @Set PATH=$(OLDPATH)

