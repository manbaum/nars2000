                /* (-1) + R * 2                                                                                   */\
                lpMemRes[uRes] = AddHC##N##F_RE (confpof_N1##suf,                                                   \
                                                 MulHC##N##F_RE (lpatRht->aplHC##N##F,                              \
                                                                 lpatRht->aplHC##N##F));                            \
