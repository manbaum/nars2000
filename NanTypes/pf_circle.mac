            fp##cho##f_acos  (*(fpcho_t *) &aplTmp[0], &lpatRht->aplHC8F);                                          \
            lpMemRes[uRes] = aplTmp[0];                                                                             \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case  -3:       /* atan (R)                                                                               */\
