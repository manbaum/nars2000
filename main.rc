IDR_SMMENU MENU DISCARDABLE
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Load...",                    IDM_LOAD_WS     , INACTIVE GRAYED
        MENUITEM "&Xload...",                   IDM_XLOAD_WS    , INACTIVE GRAYED
        MENUITEM "&Copy...",                    IDM_COPY_WS     , INACTIVE GRAYED
        MENUITEM "Pcopy...",                    IDM_PCOPY_WS    , INACTIVE GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Save",                       IDM_SAVE_WS
        MENUITEM "Save &As...",                 IDM_SAVE_AS_WS  , INACTIVE GRAYED
        MENUITEM "&Drop",                       IDM_DROP_WS     , INACTIVE GRAYED
        MENUITEM "D&uplicate",                  IDM_DUP_WS      , INACTIVE GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Print\tCtrl+P",              IDM_PRINT_WS
        MENUITEM SEPARATOR
        MENUITEM "&Exit",                       IDM_EXIT
    END
    POPUP "&Edit"
    BEGIN
#include "edit.rch"
    END
    POPUP "&Window"
    BEGIN
        MENUITEM "&Cascade\tShift+F6",          IDM_CASCADE
        MENUITEM "Tile &Horizontally\tShift+F4",IDM_TILE_HORZ
        MENUITEM "Tile &Vertically\tShift+F5",  IDM_TILE_VERT
        MENUITEM "&Arrange Icons",              IDM_ARRANGE_ICONS
    END
#ifndef DEBUG
    POPUP "&Options"
    BEGIN
        MENUITEM "&Session Manager Font",       IDM_SMFONT
        MENUITEM "&Printer Font",               IDM_PRFONT
        MENUITEM "&Tab Font",                   IDM_TCFONT
        MENUITEM "&Function Edit Font",         IDM_FEFONT
        MENUITEM "&Matrix Edit Font",           IDM_MEFONT
        MENUITEM "&Vector Edit Font",           IDM_VEFONT
    END
#endif
    POPUP "&Help"
    BEGIN
        MENUITEM "&Contents...",                IDM_HELP_CONTENTS, INACTIVE GRAYED // ***FIXME***
        MENUITEM "Check for &Updates",          IDM_UPDATES
        MENUITEM "&About",                      IDM_ABOUT
    END
END

IDR_FEMENU MENU DISCARDABLE
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Exit",                       IDM_EXIT
    END
    POPUP "&Edit"
    BEGIN
#include "edit.rch"
        MENUITEM SEPARATOR
        MENUITEM "&Localize",                   IDM_LOCALIZE
        MENUITEM "Unl&ocalize",                 IDM_UNLOCALIZE
    END
    POPUP "&Window"
    BEGIN
        MENUITEM "&Cascade\tShift+F6",          IDM_CASCADE
        MENUITEM "Tile &Horizontally\tShift+F4",IDM_TILE_HORZ
        MENUITEM "Tile &Vertically\tShift+F5",  IDM_TILE_VERT
        MENUITEM "&Arrange Icons",              IDM_ARRANGE_ICONS
    END
#ifdef DEBUG
    POPUP "&Options"
    BEGIN
        MENUITEM "&Session Manager Font",       IDM_SMFONT
        MENUITEM "&Printer Font",               IDM_PRFONT
        MENUITEM "&Tab Font",                   IDM_TCFONT
        MENUITEM "&Function Edit Font",         IDM_FEFONT
        MENUITEM "&Matrix Edit Font",           IDM_MEFONT
        MENUITEM "&Vector Edit Font",           IDM_VEFONT
    END
#endif
    POPUP "Ob&jects"
    BEGIN
        MENUITEM "&Close\tCtrl+Q",              IDM_CLOSE_FN
        MENUITEM "&Save\tCtrl+S",               IDM_SAVE_FN
        MENUITEM "Save and &End\tCtrl+E",       IDM_SAVECLOSE_FN
////////MENUITEM "Save &As\tCtrl+A",            IDM_SAVE_FN_AS      // ***FIXME*** -- Make this work??
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Contents...",                IDM_HELP_CONTENTS
        MENUITEM "&About",                      IDM_ABOUT
    END
END


