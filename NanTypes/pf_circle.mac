                                                                                                                    \
        case   9:       /* (R++R)/2                                                                               */\
            lpMemRes[uRes].parts[0] = lpatRht->aplHC##N##F.parts[0];                                                \
                                                                                                                    \
            /* Loop through the imaginary parts                                                                   */\
            for (i = 1; i < N; i++)                                                                                 \
                lpMemRes[uRes].parts[i] = 0;                                                                        \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case   8:       /* sqrt ((-1) - R * 2)                                                                    */\
