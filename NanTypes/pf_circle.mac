             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case   1:       /* sin (R)                                                                                */\
            /* Intialize to 0                                                                                     */\
            arb##D##f_init (&aplTmp);                                                                               \
                                                                                                                    \
            arb##D##f_sin   ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                      \
            lpMemRes[uRes] = aplTmp;                                                                                \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case   0:       /* sqrt (1 - R * 2)                                                                       */\
            /*  1 - R * 2                                                                                         */\
            aplMul = MulBA##N##F_RE (lpatRht->aplBA##N##F,                                                          \
                                     lpatRht->aplBA##N##F);                                                         \
            aplSub = SubBA##N##F_RE (conbaof_1##suf,                                                                \
                                     aplMul);                                                                       \
            /* sqrt (1 - R * 2)                                                                                   */\
            lpMemRes[uRes] = SqrtBAxF_RE (*(LPAPLBA8F) &aplSub, N)suf;                                              \
                                                                                                                    \
            /* We no longer need this storage                                                                     */\
            arb##D##f_clear (&aplSub);                                                                              \
            arb##D##f_clear (&aplMul);                                                                              \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case  -1:       /* asin (R)                                                                               */\
            /* Intialize to 0                                                                                     */\
            arb##D##f_init (&aplTmp);                                                                               \
                                                                                                                    \
            arb##D##f_asin  ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                      \
            lpMemRes[uRes] = aplTmp;                                                                                \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case  -2:       /* acos (R)                                                                               */\
            /* Intialize to 0                                                                                     */\
            arb##D##f_init (&aplTmp);                                                                               \
                                                                                                                    \
            arb##D##f_acos  ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                      \
            lpMemRes[uRes] = aplTmp;                                                                                \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case  -3:       /* atan (R)                                                                               */\
            /* Intialize to 0                                                                                     */\
            arb##D##f_init (&aplTmp);                                                                               \
                                                                                                                    \
            /* ***FIXME*** -- Not atan2 ???                                                                       */\
            arb##D##f_atan  ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                      \
            lpMemRes[uRes] = aplTmp;                                                                                \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case  -4:       /* (R + 1) × sqrt ((R - 1) / (R + 1))                                                     */\
                        /* a.k.a. sqrt ((-1) + R * 2)                                                             */\
            /* If the right arg is -1, return 0                                                                   */\
            if (EqualBAxFvBAxF (lpatRht,                                                                            \
                  (LPALLTYPES) &conbaof_N1##suf,                                                                    \
                                N,                                                                                  \
                                GetQuadCT (),                                                                       \
                                WFCN))                                                                              \
                /* Initialize the result to 0                                                                     */\
                arb##D##f_init (&lpMemRes[uRes]);                                                                   \
            else                                                                                                    \
            {                                                                                                       \
                /* If the imaginary parts are zero, ...                                                           */\
                if (!IzitImaginary (ARRAY_BA##N##F, lpatRht))                                                       \
                {                                                                                                   \
                    /* R * 2                                                                                      */\
                    aplMul = MulBA##N##F_RE (lpatRht->aplBA##N##F,                                                  \
                                             lpatRht->aplBA##N##F);                                                 \
                    /*  (-1) + (R * 2)                                                                            */\
                    aplAdd = AddBA##N##F_RE (conbaof_N1##suf,                                                       \
                                             aplMul);                                                               \
                    /* sqrt ((-1) + (R * 2))                                                                      */\
                    lpMemRes[uRes] = SqrtBAxF_RE (*(LPAPLBA8F) &aplAdd, N)suf;                                      \
                                                                                                                    \
                    /* We no longer need this storage                                                             */\
                    arb##D##f_clear (&aplAdd);                                                                      \
                    arb##D##f_clear (&aplMul);                                                                      \
                } else                                                                                              \
                {                                                                                                   \
                    /*  (R - 1)                                                                                   */\
                    aplSub = SubBA##N##F_RE (lpatRht->aplBA##N##F,                                                  \
                                             conbaof_1##suf);                                                       \
                    /*  (R + 1)                                                                                   */\
                    aplAdd = AddBA##N##F_RE (lpatRht->aplBA##N##F,                                                  \
                                             conbaof_1##suf);                                                       \
                    /*  (R - 1) / (R + 1)                                                                         */\
                    aplDiv= DivBA##N##F_RE (aplSub,                                                                 \
                                            aplAdd);                                                                \
                    /* We no longer need this storage                                                             */\
                    arb##D##f_clear (&aplAdd);                                                                      \
                    arb##D##f_clear (&aplSub);                                                                      \
                                                                                                                    \
                    /* sqrt ((R - 1) / (R + 1))                                                                   */\
                    aplTmp = SqrtBAxF_RE (*(LPAPLBA8F) &aplDiv, N)suf;                                              \
                                                                                                                    \
                    /* (R + 1)                                                                                    */\
                    aplAdd = AddBA##N##F_RE (lpatRht->aplBA##N##F,                                                  \
                                             conbaof_1##suf),                                                       \
                    /* (R + 1) x sqrt ((R - 1) / (R + 1))                                                         */\
                    lpMemRes[uRes] = MulBA##N##F_RE (aplAdd,                                                        \
                                                     aplTmp);                                                       \
                    /* We no longer need this storage                                                             */\
                    arb##D##f_clear (&aplAdd);                                                                      \
                    arb##D##f_clear (&aplTmp);                                                                      \
                    arb##D##f_clear (&aplDiv);                                                                      \
                } /* End IF/ELSE                                                                                  */\
            } /* End IF/ELSE                                                                                      */\
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case  -5:       /* asinh (R)                                                                              */\
                        /* ln (R + sqrt (1 + R * 2))                                                              */\
            if (arbXf_nan_p (&lpatRht->aplBA##N##F, N))                                                             \
            {                                                                                                       \
                if (gAllowNaN)                                                                                      \
                    arb##D##f_init_set (&lpMemRes[uRes], &lpatRht->aplBA##D##F);                                    \
                else                                                                                                \
                    break;                                                                                          \
            } else                                                                                                  \
            {                                                                                                       \
                /* Intialize to 0                                                                                 */\
                arb##D##f_init  (&aplTmp);                                                                          \
                                                                                                                    \
                arb##D##f_asinh ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                  \
                lpMemRes[uRes] = aplTmp;                                                                            \
                                                                                                                    \
                /* Check for NaN                                                                                  */\
                if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                            \
                 && !gAllowNaN)                                                                                     \
                    break;                                                                                          \
            } /* End IF/ELSE */                                                                                     \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case  -6:       /* acosh (R)                                                                              */\
                        /* 2 x ln (sqrt ((R + 1) x 0.5) + sqrt ((R - 1) x 0.5))                                   */\
            if (arbXf_nan_p (&lpatRht->aplBA##N##F, N))                                                             \
            {                                                                                                       \
                if (gAllowNaN)                                                                                      \
                    arb##D##f_init_set (&lpMemRes[uRes], &lpatRht->aplBA##D##F);                                    \
                else                                                                                                \
                    break;                                                                                          \
            } else                                                                                                  \
            {                                                                                                       \
                /* Intialize to 0                                                                                 */\
                arb##D##f_init  (&aplTmp);                                                                          \
                                                                                                                    \
                arb##D##f_acosh ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                  \
                lpMemRes[uRes] = aplTmp;                                                                            \
                                                                                                                    \
                /* Check for NaN                                                                                  */\
                if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                            \
                 && !gAllowNaN)                                                                                     \
                    break;                                                                                          \
            } /* End IF/ELSE */                                                                                     \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case  -7:       /* atanh (R)                                                                              */\
                        /* 0.5 x (ln (1 + R) - ln (1 - R))                                                        */\
            if (arbXf_nan_p (&lpatRht->aplBA##N##F, N))                                                             \
            {                                                                                                       \
                if (gAllowNaN)                                                                                      \
                    arb##D##f_init_set (&lpMemRes[uRes], &lpatRht->aplBA##D##F);                                    \
                else                                                                                                \
                    break;                                                                                          \
            } else                                                                                                  \
            {                                                                                                       \
                /* Intialize to 0                                                                                 */\
                arb##D##f_init  (&aplTmp);                                                                          \
                                                                                                                    \
                arb##D##f_atanh ((LPAPLBA8F) &aplTmp, &lpatRht->aplBA8F, prec);                                  \
                lpMemRes[uRes] = aplTmp;                                                                            \
                                                                                                                    \
                /* Check for NaN                                                                                  */\
                if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                            \
                 && !gAllowNaN)                                                                                     \
                    break;                                                                                          \
            } /* End IF/ELSE */                                                                                     \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case  -8:       /* -sqrt ((-1) - R * 2)                                                                   */\
            /*  R * 2                                                                                             */\
            aplMul = MulBA##N##F_RE (lpatRht->aplBA##N##F,                                                          \
                                     lpatRht->aplBA##N##F);                                                         \
            /*  (-1) - R * 2                                                                                      */\
            aplSub = SubBA##N##F_RE (conbaof_N1##suf,                                                               \
                                     aplMul);                                                                       \
            /*  sqrt ((-1) - R * 2)                                                                               */\
            aplTmp = SqrtBAxF_RE (*(LPAPLBA8F) &aplSub, N)suf;                                                      \
                                                                                                                    \
            /* -sqrt ((-1) - R * 2)                                                                               */\
            lpMemRes[uRes] = NegBA##N##F_RE (aplTmp);                                                               \
                                                                                                                    \
            /* We no longer need this storage                                                                     */\
            arb##D##f_clear (&aplTmp);                                                                              \
            arb##D##f_clear (&aplSub);                                                                              \
            arb##D##f_clear (&aplMul);                                                                              \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case  -9:       /* (R-+R)/2                                                                               */\
            arb##N##f_init_set (&lpMemRes[uRes], &lpatRht->aplBA##N##F);                                            \
            arb_zero           (&lpMemRes[uRes].parts[0]);                                                          \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case -10:       /* +R                                                                                     */\
            lpMemRes[uRes] = ConjBA##N##F (lpatRht->aplBA##N##F);                                                   \
                                                                                                                    \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case -11:       /* 0J1 x R                                                                                */\
            lpMemRes[uRes] = MulBA##N##F_RE (conbaof_I##suf,                                                        \
                                             lpatRht->aplBA##N##F);                                                 \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        case -12:       /* *0J1 x R                                                                               */\
            lpMemRes[uRes] = ExpBA##N##F_RE (MulBA##N##F_RE (conbaof_I##suf,                                        \
                                                             lpatRht->aplBA##N##F));                                \
            /* Check for NaN                                                                                      */\
            if (arb_nan_p (&lpMemRes[uRes].parts[0])                                                                \
             && !gAllowNaN)                                                                                         \
                break;                                                                                              \
            return;                                                                                                 \
                                                                                                                    \
        default:                                                                                                    \
            break;                                                                                                  \
    } /* End SWITCH                                                                                               */\
                                                                                                                    \
    RaiseException (EXCEPTION_DOMAIN_ERROR, 0, 0, NULL);

