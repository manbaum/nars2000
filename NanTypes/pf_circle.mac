            } else                                                                                                  \
            {                                                                                                       \
                fp##cho##f_asinh (*(fpcho_t *) &aplTmp[0], &lpatRht->aplHC8F);                                      \
                lpMemRes[uRes] = aplTmp[0];                                                                         \
             } /* End IF/ELSE */                                                                                    \
                                                                                                                    \
            return;                                                                                                 \
                                                                                                                    \
        case  -6:       /* acosh (R)                                                                              */\
