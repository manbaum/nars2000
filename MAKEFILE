$(O)$(ENAME).EXE:       $(OBJS)         \
                        $(DEFFILE)      \
                        MAKEFILE
        @echo >NUL @<<$(O)main.CRF
$(OBJS)
/OUT:$(O)$(ENAME).EXE
/MAP:$(O)$(MAPFILE)
$(LIBS_WEXE) $(LIBS_EXT)
/DEF:$(DEFFILE)
<<KEEP
        @Set LIB=$(L98);$(L)
        @if exist $@ del $@
        @$(LD) $(LFLAGS_WEXE) @$(O)main.CRF
        @del $(O)$(@B).exp
        @del $(O)$(@B).lib
        @pkzip $(B)$(ENAME) $@ >NUL

$(L)$(ENAME).EXE:       $(O)$(ENAME).EXE
        @xc/r    %s      $@ >NUL
        @ff $(B)$(ENAME)

zip:
        pkzip $(ZIPNAME) makefile mk.bat mkd.bat $(A)*.bmp *.c *.h $(BC)*.hlp $(A)*.ico *.rc *.rtf *.y
